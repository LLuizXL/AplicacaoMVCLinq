@{
}
@model IEnumerable<AplicacaoMVCLinq.Models.Cliente>


<h1>Clientes Inseridos:</h1>

<div class="container">

<table class="table table-hover table-dark table-striped-columns table-responsive">
    <thead>
        <tr>
            <th>ID</th>
            <th>Nome</th>
            <th>E-mail</th>
            <th>Telefone</th>
            <th>Nascimento</th>

        </tr>

    </thead>
    <tbody>
        @foreach (var cliente in Model.OrderBy(cliente => cliente.Nome))
      {
          <tr>
            <td>@cliente.ClienteId</td>
            <td>@cliente.Nome</td>
            <td>@cliente.Email</td>
            <td>@cliente.Telefone</td>
            <td>@cliente.DataNasc</td>
        </tr>
      }
    </tbody>
</table>
</div>